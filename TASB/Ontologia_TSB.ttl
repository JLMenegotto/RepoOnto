bim:Critério.Qualif.UA01.A  rdf:type  owl:NamedIndividual , bim:Critério.Positivo ;
     bim:é.dentro.de    bim:Caderno.S.Sociais ;
     bim:é.pertencente.a    bim:Setor.TSB.SAS ;
     bim:é.aplicado.em    bim:Objetivo.02 ;
